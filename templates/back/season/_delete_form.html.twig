<form method="post" action="{{ path('app_back_season_delete', {'id': season.id}) }}" onsubmit="return confirm('Etes vous sûr de vouloir supprimer cet équipe?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ season.id) }}">
    <button class="btn btn-danger mt-1">Supprimer</button>
</form>
